PROJECTS = FiveDaemonScheduler.exe Rankings.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

FiveDaemonScheduler.exe: FiveDaemonScheduler.dpr
  $(DCC)

Rankings.dll: Rankings.dpr
  $(DCC)


